import std/core;

struct Foo {
	var value: int;

	func constructor(this: &&Foo, value: int) {
		this.value = value;
	}

	func destructor(this: &&Foo) {}

	operator <=>(this: Foo, other: Foo) -> int {
		return this.value - other.value;
	}
}

func main() {
	assert(Foo(1) == Foo(1));
	assert(Foo(0) != Foo(1));
	assert(Foo(0) < Foo(1));
	assert(Foo(1) <= Foo(1));
	assert(Foo(1) > Foo(0));
	assert(Foo(1) >= Foo(1));
}
