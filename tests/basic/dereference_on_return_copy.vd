import std/core;
import std/string;
import std/io;

struct Foo {
	var x: int;

	func constructor(this: &&Foo) {
		print("+");
	}

	func constructor(this: &&Foo, other: Foo) {
		print("=");
	}

	func destructor(this: &&Foo) {
		print("-");
	}
}

func bar(x: &&Foo) -> Foo {
	return x;
}

func main() {
	var x: Foo;
	discard bar(&&x);
}
