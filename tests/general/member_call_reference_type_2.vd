//> output "const-ref"

import std/core;
import std/string;
import std/io;

struct Foo {
	var x: int;

	func constructor(this: &&Foo) -> void = default;
	func constructor(this: &&Foo, other: Foo) -> void = default;
	func destructor(this: &&Foo) -> void = default;

	func foo(this: &&Foo) {
		println("var-ref");
	}

	func foo(this: &Foo) {
		println("const-ref");
	}

	func foo(this: Foo) {
		println("base");
	}
}

func main() {
	const x = Foo();
	x.foo();
}
