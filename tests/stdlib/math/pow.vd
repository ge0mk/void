import std/core;
import std/math;

func main() -> void {
	assert(abs!<f64>(pow!<f64>(2.0, 2.0) - 4.0) < 0.0001);
	assert(abs!<f64>(pow!<f64>(3.0, 2.0) - 9.0) < 0.0001);
	assert(abs!<f64>(pow!<f64>(4.0, 2.0) - 16.0) < 0.0001);

	assert(abs!<f64>(pow!<f64>(4.0, 0.5) - 2.0) < 0.0001);
	assert(abs!<f64>(pow!<f64>(9.0, 0.5) - 3.0) < 0.0001);
	assert(abs!<f64>(pow!<f64>(16.0, 0.5) - 4.0) < 0.0001);
}
