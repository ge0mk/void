import std/core;
import std/list;

func main() {
	var a = List!<int>();

	must a.append(1);
	must a.append(2);
	must a.append(3);
	must a.append(4);
	must a.append(5);

	var b: List!<int> = must a[1u..4u];
	assert(b.size() == 3u);
	assert((must b[0u]) == 2);
	assert((must b[1u]) == 3);
	assert((must b[2u]) == 4);
}
