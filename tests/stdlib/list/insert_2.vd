import std/core;
import std/list;

func main() {
	var a = List!<int>();
	var b = List!<int>();

	must a.append(1);
	must a.append(2);
	must a.append(3);

	must b.append(4);
	must b.append(5);
	must b.append(6);

	must a.insert(2u, b);

	assert((must a[1u]) == 2);
	assert((must a[2u]) == 4);
	assert((must a[3u]) == 5);
}
