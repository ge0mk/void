	func constructor(this: &&Foo) {
		println("+");
	}

	func destructor(this: &&Foo) {
		println("-");
	}
}

func foo() -> Result!<void, int> {
	throw 0;
}

func bar() -> Result!<void, int> {
	var x: Foo;
